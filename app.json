{
  "expo": {
    "name": "Envest",
    "description": "Smartest Crypto Investing Platform",
    "slug": "envest-crypto-investing",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "envest",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/splash.png",
      "config": {
        "googleSignIn": {
          "behavior": "system",
          "clientId": "507053804224-dvt66inccsp1ah0a75dni5lq5812cohd.apps.googleusercontent.com"
        }
      }
    }
  }
}
